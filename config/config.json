{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "qr-code-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/qrCode/QrCodeCommandSet.js",
          "manifest": "./src/extensions/qrCode/QrCodeCommandSet.manifest.json"
        }
      ]
    },
    "qr-code-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/qrCodeWp/QrCodeWpWebPart.js",
          "manifest": "./src/webparts/qrCodeWp/QrCodeWpWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "QrCodeCommandSetStrings": "lib/extensions/qrCode/loc/{locale}.js",
    "QrCodeWpWebPartStrings": "lib/webparts/qrCodeWp/loc/{locale}.js"
  }
}
